@model IEnumerable<Sahayee.Models.DB.Course>

<table class="table">
    <thead>
        <tr>
            <th>Name</th>
            <th>Category</th>
            <th>Location</th>
            <th>Institution</th>
            <th>Duration</th>
            <th>Actions</th>
        </tr>
    </thead>
    <tbody>
        @foreach (var job in Model)
        {
            <tr>
                <td>@job.Name</td>
                <td>@job.Category</td>
                <td>@job.Location</td>
                <td>@job.Institution</td>
                <td>@job.Duration</td>
                <td>
                    <button class="btn btn-warning" onclick="openModal('@job.Id')">Edit</button>
                    <a href="/Course/AdminApplicantDetails?courseId=@job.Id" class="btn btn-warning">Details</a>
                   @* <button class="btn btn-danger" onclick="openDeleteModal('@job.Id')">Delete</button>*@
                </td>
            </tr>
        }
    </tbody>
</table>
